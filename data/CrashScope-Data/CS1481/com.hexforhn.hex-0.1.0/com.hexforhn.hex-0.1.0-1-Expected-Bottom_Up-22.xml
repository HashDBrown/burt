" /><node index="5" text="." resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[674,1792][693,1794]" /></node><node index="5" text="We understand that certain add-ons are invaluable to your workflow. So, to help you decide the best time to update your version of Thunderbird, here’s a simple way to discover if the extensions you currently have installed are compatible with Thunderbird 102." resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[39,1792][1042,1794]" /><node index="6" text="Install “Addon Compatibility Check For TB 102”" resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[39,1792][1042,1794]" /><node index="7" text="" resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[39,1792][1042,1794]"><node index="0" text="How do you check the compatibility of your add-ons? By installing an add-on! While it doesn’t have the most creative name, “" resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[39,1792][1031,1794]" /><node index="1" text="Addon Compatibility Check For TB 102" resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="true" enabled="true" focusable="true" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[39,1792][963,1794]" /><node index="2" text="” does its job considerably well." resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[107,1792][690,1794]" /></node><node index="8" text="(This extension works with Thunderbird 68 and newer.)" resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[39,1792][1042,1794]" /><node index="9" text="Click the icon that looks like a puzzle to instantly check add-on compatibility" resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[39,1792][1042,1794]"><node index="0" text="Addon Compatibility Check For TB 102 icon in Thunderbird 102" resource-id="" class="android.view.View" package="com.hexforhn.hex" content-desc="" checkable="false" checked="false" clickable="true" enabled="true" focusable="true" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[39,1792][1042,1794]" /><node index="1" text="Click the icon that looks like a puzzle to instantly check add-on compatibility" resource-id="" class="andro